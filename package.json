{
  "name": "Paysmart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
      "svg-icons": "svgr --native src/assets/svg --out-dir src/assets/auto-generated-svg-icons",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assemble-android-debug:unix": "cd android && ./gradlew assembleDebug",
    "assemble-android-debug:win": "cd android && gradlew.bat assembleDebug",
    "assemble-android-debug": "node -e \"const os=require('os'); const isWin=os.platform().startsWith('win'); const cmd=isWin? 'cd android && gradlew.bat assembleDebug' : 'cd android && ./gradlew assembleDebug'; require('child_process').execSync(cmd, {stdio:'inherit'});\"",
    "build:debug": "npm run bundle-android && npm run assemble-android-debug"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.83.1",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/drawer": "^7.7.10",
    "@react-navigation/native-stack": "^7.9.0",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-worklets": "^0.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
